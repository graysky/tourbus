<% @header_title = date_venue_title(@show) + " / Photos" %>
<% @page_title = long_show_title(@show) + " / Photos" %>

<div id="main">

<%= render :partial => "profile_header", :locals => { :front_page => false } %>

  <%= render(
  :partial => "shared/photo_preview", 
  :locals => 
    {
    :photos => @photos,
    :type => Photo.Show,
    :id => @show,
    :full => true
    }) 
  %>
  
</div>

<%= render :partial => "profile_sidebar", :locals => { :show_fans => true } %>